    :extractor_in_api_module "util.xqm" ;
    :code_ref <https://github.com/dracor-org/dracor-api/blob/665ea3f07c3f0ab83566440436691ed73957b263/modules/util.xqm#L1005> ;
    :extractor_in_api_function "dutil:get-play-info" ;
