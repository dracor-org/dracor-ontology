    rdfs:comment """Number of stage directions in a corpus. The value is the number of <tei:stage> elements in a collection representing a corpus in the underlying eXist database."""@en;
    :extractor_in_api_module "api.xqm" ;
    :extractor_in_api_function "local:get-corpus-metrics" ;
    :code_ref <https://github.com/dracor-org/dracor-api/blob/665ea3f07c3f0ab83566440436691ed73957b263/modules/api.xqm#L208> ;
