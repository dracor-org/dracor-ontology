    :feature_id "P4" ;
    :xpath "substring-after(/tei:TEI/tei:standOff/tei:listRelation/tei:relation[@name='wikidata'][1]/@passive/string(),'http://www.wikidata.org/entity/')" ;
    :operation_id "list-corpus-content" , "corpus-metadata" , "corpus-metadata-csv-endpoint" , "play-info" ;
    :feature_name "play_wikidata_id" ;
    :documentation <https://dracor.org/doc/odd#play_wikidata_id> .


:play_title
    a owl:DatatypeProperty, rdf:Property ;
    rdfs:domain :Play ;
    rdfs:range  rdfs:Literal;
    rdfs:label "play title"@en ;
    rdfs:comment """(Main) title of the play. Normally, the title is in the language of the corpus. The respective function extracts the text of the first element <tei:title> in <tei:titleStmt> in the <tei:teiHeader>."""@en ;
