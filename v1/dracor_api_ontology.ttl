    rdfs:domain :Play ;
    rdfs:range  rdfs:Literal;
    rdfs:label "play id"@en ;
    rdfs:comment "DraCor ID of the play, e.g. 'ger000171'. In the TEI source file the DraCor ID is contained in the attribute @xml:id on the root element <tei:TEI>."@en ;
