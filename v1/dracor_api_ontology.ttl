    rdfs:range  rdfs:Literal;
    rdfs:label "play wikidata id"@en ;
    rdfs:comment """Wikidata ID of the play. Identifier ('Q-Number') of the item on Wikidata, e.g. 'Q466333'. In the TEI source document the information is contained in the <tei:standOff> element as a <tei:relation> with the attribute @name 'wikidata' as the full URI, e.g. 'http://www.wikidata.org/entity/Q466333'."""@en ;
    :extractor_in_api_module "util.xqm" ;
    :extractor_in_api_function "dutil:get-play-wikidata-id" ;
