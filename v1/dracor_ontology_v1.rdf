  <rdf:Property rdf:about="https://lod.dracor.org/ontology/v1/play_wikidata_id">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
    <rdfs:domain rdf:resource="https://lod.dracor.org/ontology/v1/Play"/>
    <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
    <rdfs:label xml:lang="en">Wikidata ID of the Play</rdfs:label>
    <ns0:feature_id>P4</ns0:feature_id>
    <ns0:xpath>substring-after(/tei:TEI/tei:standOff/tei:listRelation/tei:relation[@name='wikidata'][1]/@passive/string(),'http://www.wikidata.org/entity/')</ns0:xpath>
  </rdf:Property>

  <rdf:Property rdf:about="https://lod.dracor.org/ontology/v1/play_title">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
    <rdfs:domain rdf:resource="https://lod.dracor.org/ontology/v1/Play"/>
    <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
    <rdfs:label xml:lang="en">Title of the Play</rdfs:label>
    <ns0:feature_id>P5</ns0:feature_id>
