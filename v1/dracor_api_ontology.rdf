    <ns0:operation_id>corpus-metadata-csv-endpoint</ns0:operation_id>
    <ns0:operation_id>play-info</ns0:operation_id>
    <ns0:feature_name>play_subtitle</ns0:feature_name>
  </owl:DatatypeProperty>

