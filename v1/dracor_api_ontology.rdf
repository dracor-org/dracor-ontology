    <ns0:code_ref rdf:resource="https://github.com/dracor-org/dracor-api/blob/665ea3f07c3f0ab83566440436691ed73957b263/modules/util.xqm#L1007"/>
    <ns0:feature_id>P5</ns0:feature_id>
    <ns0:xpath>/tei:TEI//tei:fileDesc/tei:titleStmt/tei:title[1]/normalize-space()</ns0:xpath>
    <ns0:xpath>/tei:TEI/tei:teiHeader/tei:fileDesc[1]/tei:titleStmt/tei:title[not(@type = 'sub') and not(@xml:lang or ./ancestor::tei:TEI/@xml:lang = @xml:lang)]/normalize-space()</ns0:xpath>
    <ns0:operation_id>list-corpus-content</ns0:operation_id>
    <ns0:operation_id>corpus-metadata</ns0:operation_id>
    <ns0:operation_id>corpus-metadata-csv-endpoint</ns0:operation_id>
    <ns0:operation_id>play-info</ns0:operation_id>
    <ns0:feature_name>play_title</ns0:feature_name>
  </owl:DatatypeProperty>

  <owl:DatatypeProperty rdf:about="https://dracor.org/ontology/dracor-api-ontology#play_subtitle">
