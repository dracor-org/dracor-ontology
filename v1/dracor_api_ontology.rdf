  <owl:DatatypeProperty rdf:about="https://dracor.org/ontology/dracor-api-ontology#play_wikidata_id">
    <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property"/>
    <rdfs:domain rdf:resource="https://dracor.org/ontology/dracor-api-ontology#Play"/>
    <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
    <rdfs:label xml:lang="en">play wikidata id</rdfs:label>
    <rdfs:comment xml:lang="en">Wikidata ID of the play. Identifier ('Q-Number') of the item on Wikidata, e.g. 'Q466333'. In the TEI source document the information is contained in the &lt;tei:standOff&gt; element as a &lt;tei:relation&gt; with the attribute @name 'wikidata' as the full URI, e.g. 'http://www.wikidata.org/entity/Q466333'.</rdfs:comment>
    <ns0:extractor_in_api_module>util.xqm</ns0:extractor_in_api_module>
    <ns0:extractor_in_api_function>dutil:get-play-wikidata-id</ns0:extractor_in_api_function>
    <ns0:code_ref rdf:resource="https://github.com/dracor-org/dracor-api/blob/665ea3f07c3f0ab83566440436691ed73957b263/modules/util.xqm#L895-L906"/>
    <ns0:feature_id>P4</ns0:feature_id>
    <ns0:xpath>substring-after(/tei:TEI/tei:standOff/tei:listRelation/tei:relation[@name='wikidata'][1]/@passive/string(),'http://www.wikidata.org/entity/')</ns0:xpath>
    <ns0:operation_id>list-corpus-content</ns0:operation_id>
    <ns0:operation_id>corpus-metadata</ns0:operation_id>
    <ns0:operation_id>corpus-metadata-csv-endpoint</ns0:operation_id>
    <ns0:operation_id>play-info</ns0:operation_id>
    <ns0:feature_name>play_wikidata_id</ns0:feature_name>
  </owl:DatatypeProperty>

  <owl:DatatypeProperty rdf:about="https://dracor.org/ontology/dracor-api-ontology#play_title">
    <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property"/>
    <rdfs:domain rdf:resource="https://dracor.org/ontology/dracor-api-ontology#Play"/>
    <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
    <rdfs:label xml:lang="en">play title</rdfs:label>
    <rdfs:comment xml:lang="en">(Main) title of the play. Normally, the title is in the language of the corpus. The respective function extracts the text of the first element &lt;tei:title&gt; in &lt;tei:titleStmt&gt; in the &lt;tei:teiHeader&gt;.</rdfs:comment>
    <ns0:extractor_in_api_module>util.xqm</ns0:extractor_in_api_module>
    <ns0:extractor_in_api_function>dutil:get-titles</ns0:extractor_in_api_function>
