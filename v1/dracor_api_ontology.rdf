    <ns0:xpath>/tei:TEI//tei:titleStmt/tei:title[@type='sub'][1]/normalize-space()</ns0:xpath>
    <ns0:xpath>/tei:TEI/tei:teiHeader/tei:fileDesc[1]/tei:titleStmt/tei:title[@type = 'sub' and not(@xml:lang or ./ancestor::tei:TEI/@xml:lang = @xml:lang)]/normalize-space()</ns0:xpath>
    <ns0:operation_id>list-corpus-content</ns0:operation_id>
